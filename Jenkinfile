pipeline{
agent any
stages{
stage ('compile stage')
{
steps{
withMaven(maven : 'maven 3.5.2'){
sh 'mvn clean compile'
}
}
}
stage ('testing stage')
{
steps{
withMaven(maven : 'maven 3.5.2'){
sh 'mvn test'
}
}
}
stage('deploying stage')
{
steps{
withMaven(maven : 'maven 3.5.2')
{
sh 'mvn deploy'
}
}
}
}
}

